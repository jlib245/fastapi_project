# .env.example

# 데이터베이스 연결 URL
# 실제 개발 환경에서는 이 값을 그대로 사용해도 괜찮지만,
# 민감한 정보(예: 비밀번호 포함된 DB URL)는 별도로 관리해야 합니다.
DATABASE_URL=./db/fastapi_project.db

# 디버그 모드 (개발 환경에서 True로 설정, 프로덕션에서는 False 권장)
DEBUG=True

# 애플리케이션의 시크릿 키 (실제 값은 직접 설정하거나 안전한 방법으로 관리)
SECRET_KEY=your_secret_key_here

# 기타 환경변수 예시
# API_KEY=your_api_key_here
